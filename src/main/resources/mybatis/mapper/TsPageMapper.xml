            <if test="bookId != null">
                AND `book_id` = #{bookId}
            </if>
            <if test="loaclImageUrl != null">
                AND `loacl_image_url` = #{loaclImageUrl}
            </if>
--             AND page_image is null
--                AND page_image is not null
--             AND loacl_image_url is null
            <if test="start != null and end != null">
                limit ${start-1}, ${end-start+1}
            </if>
        </where>
    </select>

