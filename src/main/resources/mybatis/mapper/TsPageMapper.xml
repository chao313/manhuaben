        INSERT INTO `ts_page`(
        `id`,
        `page_url`,
        `page_image_url`,
        `page_image`,
        `page_index`,
        `book_id`,
        `loacl_image_url`
        )VALUES
        <foreach collection="vos" item="item" separator=",">
            (
            #{item.id},
            #{item.pageUrl},
            #{item.pageImageUrl},
            #{item.pageImage},
            #{item.pageIndex},
            #{item.bookId},
            #{item.loaclImageUrl}
            )
