    <insert id="insert"
            parameterType="demo.spring.boot.demospringboot.mybatis.vo.TsPageVo">
        INSERT INTO `ts_page`(
        `id` ,
        `page_url` ,
        `page_image_url` ,
        `page_image` ,
        `page_index` ,
        `book_id` ,
        `loacl_image_url`
        )VALUE(
          #{id}  ,
          #{pageUrl}  ,
          #{pageImageUrl}  ,
          #{pageImage}  ,
          #{pageIndex}  ,
          #{bookId}  ,
          #{loaclImageUrl}
        )
    </insert>

    <insert id="inserts"
